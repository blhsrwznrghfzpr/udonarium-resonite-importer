# ResoniteLink connection settings
# Copy this file to .env and fill in your values

# ResoniteLink port (required)
# This port changes each time you start a Resonite world
# Check the ResoniteLink component in Resonite for the current port
RESONITELINK_PORT=

# ResoniteLink host (optional, default: localhost)
# RESONITELINK_HOST=localhost

# Integration test switch (set true to run test:integration)
# RESONITE_LINK_AVAILABLE=false
